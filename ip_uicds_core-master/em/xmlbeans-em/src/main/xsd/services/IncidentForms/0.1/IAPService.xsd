  <import namespace="http://uicds.org/ICSFormCommon" schemaLocation="../../../services/IncidentForms/0.1/ICSFormCommon.xsd"/>
  <element name="CreateIAPRequest">
    <complexType>
      <all>
        <element name="incidentId" type="string" minOccurs="0"/>
